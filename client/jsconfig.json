{
  "include": [
    "./src/**/*.vue",
    "./src/**/*.js",
    "./vite-env.d.ts"
  ],
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "jsx": "preserve",
    "allowJs": true,
    "checkJs": true,
    "typeRoots": [
      "./node_modules/@types",
      "./"
    ]
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}